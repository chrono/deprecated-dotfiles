<tskel:before>
let rows = input('Rows: ')
let cols = input('Columns: ')
if rows == '' || cols == ''
    let b:tskelRows = '| <+CURSOR+> |'
else
    let b:tskelRows = TSkeletonInsertTable(rows, cols, '| ', ' |', ' | ')
endif
</tskel:before>
<tskel:after>
unlet b:tskelRows
</tskel:after>
<+CURSOR+><+b:tskelRows+>
